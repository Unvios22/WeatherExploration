<Project Sdk="Godot.NET.Sdk/4.3.0">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <TargetFramework Condition=" '$(GodotTargetPlatform)' == 'android' ">net7.0</TargetFramework>
    <TargetFramework Condition=" '$(GodotTargetPlatform)' == 'ios' ">net8.0</TargetFramework>
    <EnableDynamicLoading>true</EnableDynamicLoading>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Chickensoft.AutoInject" Version="2.1.0" />
    <PackageReference Include="Chickensoft.GodotNodeInterfaces" Version="2.2.4" />
    <PackageReference Include="Chickensoft.Introspection" Version="1.3.0" />
    <PackageReference Include="Chickensoft.Introspection.Generator" Version="1.3.0" />
    <PackageReference Update="GodotSharp" Version="4.3.0-dev.6" />
    <PackageReference Update="Godot.SourceGenerators" Version="4.3.0-dev.6" />
  </ItemGroup>
  <ItemGroup>
    <Content Include="Shader\ComputeShader.glsl" />
  </ItemGroup>
</Project>